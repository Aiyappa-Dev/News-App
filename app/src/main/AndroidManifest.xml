<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aimright.admin.demoapp1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/infomo_logo_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/infomo_logo_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.aimright.admin.demoapp1.Activities.Youtube"></activity>
        <activity android:name="com.aimright.admin.demoapp1.Activities.AboutApplication" />
        <activity
            android:name="com.aimright.admin.demoapp1.Activities.WelcomeScreen"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.aimright.admin.demoapp1.Activities.UserInterest"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.aimright.admin.demoapp1.Activities.AdPreferences"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.aimright.admin.demoapp1.Activities.SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.aimright.admin.demoapp1.Activities.NewsDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.aimright.admin.demoapp1.Activities.NewsActivity"
            android:screenOrientation="portrait">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN" /> -->


            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter> -->
        </activity>
        <activity
            android:name="com.aimright.admin.demoapp1.Activities.UserProfile"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" />

        <service
            android:name="com.aimright.admin.demoapp1.Services.MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>