<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.AboutApplication">
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textColor="@color/black"
    android:text="App Version :"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    android:layout_margin="10dp"
    android:id="@+id/app_version_text"/>
    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="App Version :"
    app:layout_constraintStart_toEndOf="@+id/app_version_text"
    app:layout_constraintTop_toTopOf="parent"
    android:layout_margin="10dp"
    android:id="@+id/app_version"/>

    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textColor="@color/black"
    android:text="SDK Version :"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@id/app_version_text"
    android:layout_marginTop="5dp"
        android:layout_marginStart="10dp"
    android:id="@+id/sdk_version_text"/>
    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="sdk Version :"
    app:layout_constraintStart_toEndOf="@+id/sdk_version_text"
    app:layout_constraintTop_toBottomOf="@+id/app_version"
    android:layout_marginTop="5dp"
        android:layout_marginStart="10dp"
    android:id="@+id/sdk_version"/>
     <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textColor="@color/black"
    android:text="Ads Enable :"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@id/sdk_version_text"
    android:layout_marginTop="5dp"
        android:layout_marginStart="10dp"
    android:id="@+id/ads_enable_text"/>
    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="sdk Version :"
    app:layout_constraintStart_toEndOf="@+id/ads_enable_text"
    app:layout_constraintTop_toBottomOf="@+id/sdk_version"
    android:layout_marginTop="5dp"
    android:layout_marginStart="10dp"
    android:id="@+id/ads_enable"/>

    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textColor="@color/black"
    android:text="Publisher Name:"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@id/ads_enable"
    android:layout_marginTop="5dp"
        android:layout_marginStart="10dp"
    android:id="@+id/publisher_name_text"/>
    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="sdk Version :"
    app:layout_constraintStart_toEndOf="@+id/publisher_name_text"
    app:layout_constraintTop_toBottomOf="@+id/ads_enable"
    android:layout_marginTop="5dp"
    android:layout_marginStart="10dp"
    android:id="@+id/publisher_name"/>

    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textColor="@color/black"
    android:text="Stage Name:"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@id/publisher_name_text"
    android:layout_marginTop="5dp"
        android:layout_marginStart="10dp"
    android:id="@+id/stage_name_text"/>
    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="sdk Version :"
    app:layout_constraintStart_toEndOf="@+id/stage_name_text"
    app:layout_constraintTop_toBottomOf="@+id/publisher_name"
    android:layout_marginTop="5dp"
    android:layout_marginStart="10dp"
    android:id="@+id/stage_name"/>

</androidx.constraintlayout.widget.ConstraintLayout>